Subject: [PATCH 45/71] Update vanilla command permissions

---
 rainbow/src/main/java/org/projectrainbow/mixins/MixinCommandBase.java | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/rainbow/src/main/java/org/projectrainbow/mixins/MixinCommandBase.java b/rainbow/src/main/java/org/projectrainbow/mixins/MixinCommandBase.java
index adc3e38..57b3c42 100644
--- a/rainbow/src/main/java/org/projectrainbow/mixins/MixinCommandBase.java
+++ b/rainbow/src/main/java/org/projectrainbow/mixins/MixinCommandBase.java
@@ -16,7 +16,7 @@ public class MixinCommandBase {
     @Inject(method = "checkPermission", at = @At("HEAD"), cancellable = true)
     private void canUse(MinecraftServer server, ICommandSender commandSender, CallbackInfoReturnable<Boolean> callbackInfo){
         if (commandSender instanceof MC_Player) {
-            callbackInfo.setReturnValue(((MC_Player)commandSender).hasPermission("rainbow." + ((ICommand)this).getCommandName()));
+            callbackInfo.setReturnValue(((MC_Player)commandSender).hasPermission("rails." + ((ICommand)this).getCommandName()));
         }
     }
 }
